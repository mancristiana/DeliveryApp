<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="page-container" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.kea.swc.cadd.delivery.view.LocationOverviewController">
   <center>
      <TableView fx:id="locationTable">
         <columns>
            <TableColumn fx:id="cityNameColumn" text="City name" />
            <TableColumn fx:id="storageNameColumn" text="Storage name" />
            <TableColumn fx:id="priceColumn" text="Price (RON/ton)" />
            <TableColumn fx:id="editColumn" prefWidth="50" sortable="false" text="Edit" />
         </columns>
      </TableView>
   </center>
   <top>
      <Label id="pagetitle" text="Locations" />
   </top>
</BorderPane>
